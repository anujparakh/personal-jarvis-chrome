{
    "name": "jarvis",
    "description": "Personal Utility Extension",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": "Ctrl+Shift+O",
            "description": "Open ticket"
        }
    },
    "permissions": [
        "activeTab",
        "tabs",
        "scripting",
        "webNavigation"
    ],
    "icons": {
        "128": "res/icon.png"
    }
}